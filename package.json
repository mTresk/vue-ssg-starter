{
  "name": "vue-ssg-starter",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "run-s generate-styles-entry dev:vite",
    "dev:vite": "vite",
    "build": "run-s fix-all generate-styles-entry build:vite cleanup post-process",
    "build:vite": "vite-ssg build",
    "preview": "vite preview",
    "fonts": "tsx ./src/utils/tasks/fonts.ts",
    "generate-styles-entry": "tsx ./src/utils/tasks/generate-styles-entry.ts",
    "cleanup": "tsx ./src/utils/tasks/cleanup.ts",
    "post-process": "tsx ./src/utils/tasks/post-process.ts",
    "lint:js": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:css": "stylelint \"**/*.{css,scss,pcss}\"",
    "lint:css:fix": "stylelint \"**/*.{css,scss,pcss}\" --fix",
    "px-to-rem": "tsx ./src/utils/tasks/px-to-rem.ts",
    "lint": "run-s lint:js lint:css",
    "lint:fix": "run-s lint:js:fix lint:css:fix",
    "fix-all": "run-s px-to-rem lint:fix"
  },
  "dependencies": {
    "inputmask": "^5.0.9",
    "js-datepicker": "^5.18.4",
    "lightgallery": "^2.9.0-beta.1",
    "nice-select2": "^2.4.1",
    "nouislider": "^15.8.1",
    "smooth-scrollbar": "^8.8.4",
    "swiper": "^11.2.10",
    "tippy.js": "^6.3.7",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^24.3.1",
    "@unhead/vue": "^2.0.14",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "cheerio": "^1.1.2",
    "eslint": "^9.34.0",
    "eslint-plugin-format": "^1.0.1",
    "fontmin": "^2.0.3",
    "glob": "^11.0.3",
    "js-beautify": "^1.15.4",
    "npm-run-all2": "^8.0.4",
    "sass": "^1.92.0",
    "sharp": "^0.34.3",
    "stylelint": "^16.23.1",
    "stylelint-config-recess-order": "^7.2.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2",
    "vite": "^7.1.4",
    "vite-plugin-image-optimizer": "^2.0.2",
    "vite-ssg": "^28.1.0",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.6"
  }
}
